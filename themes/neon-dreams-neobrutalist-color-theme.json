  "tokenColors": [
    // Comments - Balanced purple
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line",
        "comment.block"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8a6a8a"
      }
    },
    {
      "name": "Comment Keywords",
      "scope": [
        "comment.line.double-slash keyword",
        "comment.block keyword"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#aa8aaa"
      }
    },
    // Strings - Radiant/hacky green variants with more pop
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#00ff99"
      }
    },
    {
      "name": "Template String",
      "scope": [
        "string.template",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#33ffbb"
      }
    },
    {
      "name": "String Interpolation",
      "scope": [
        "meta.template.expression",
        "string.template meta.embedded"
      ],
      "settings": {
        "foreground": "#dd77dd"
      }
    },
    {
      "name": "Regular Expression",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#dd99bb"
      }
    },
    // Numbers and Constants - Enhanced purple with more pop
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.binary",
        "constant.numeric.octal"
      ],
      "settings": {
        "foreground": "#ff55ff"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#ff77ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Null/Undefined",
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.none"
      ],
      "settings": {
        "foreground": "#dd88cc",
        "fontStyle": "italic"
      }
    },
    // Keywords - Enhanced pink with more pop
    {
      "name": "Control Keywords",
      "scope": [
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop"
      ],
      "settings": {
        "foreground": "#ff77dd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Keywords",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ff77dd"
      }
    },
    {
      "name": "Import/Export Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "foreground": "#ff55ff",
        "fontStyle": "italic"
      }
    },
    // Operators - Blue for balance
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#88ccdd"
      }
    },
    {
      "name": "Assignment Operator",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#99ddee",
        "fontStyle": "bold"
      }
    },
    // Functions - Blue variations with more pop
    {
      "name": "Function Declaration",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#99ddff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#aaeeff"
      }
    },
    {
      "name": "Method",
      "scope": [
        "entity.name.function.member",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#88ccee"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": [
        "support.function",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#55ffbb"
      }
    },
    // Variables - Clean whites
    {
      "name": "Variable Declaration",
      "scope": [
        "meta.definition.variable variable.other",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#e8d8e8"
      }
    },
    {
      "name": "Variable Usage",
      "scope": [
        "variable.other"
      ],
      "settings": {
        "foreground": "#d8c8d8"
      }
    },
    {
      "name": "Constant Variable",
      "scope": [
        "variable.other.constant",
        "constant.other"
      ],
      "settings": {
        "foreground": "#99ddee",
        "fontStyle": "bold"
      }
    },
    // Parameters and Arguments - Enhanced warm orange to match function calls
    {
      "name": "Function Parameter",
      "scope": [
        "variable.parameter",
        "meta.function.parameters variable"
      ],
      "settings": {
        "foreground": "#ffaa44",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Destructured Parameter",
      "scope": [
        "meta.object-binding-pattern-variable variable.parameter",
        "meta.array-binding-pattern-variable variable.parameter"
      ],
      "settings": {
        "foreground": "#ffcc66"
      }
    },
    // Properties and Object Members - Clean white
    {
      "name": "Object Property",
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "Object Property Access",
      "scope": [
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    // Classes and Types - Enhanced bright red with more pop
    {
      "name": "Class Name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#ff4466",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Interface",
      "scope": [
        "entity.name.interface",
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "#77ddcc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": [
        "support.type",
        "entity.name.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#88ccdd"
      }
    },
    {
      "name": "Generic Type",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#99ddee",
        "fontStyle": "italic"
      }
    },
    // HTML/JSX/TSX - Balanced colors
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#dd77bb"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#88ccdd"
      }
    },
    {
      "name": "JSX Component",
      "scope": [
        "support.class.component",
        "entity.name.tag.js",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#cc77ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Attribute",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#99ddee"
      }
    },
    // CSS - Balanced colors
    {
      "name": "CSS Property",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#88ccdd"
      }
    },
    {
      "name": "CSS Value",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "name": "CSS Selector",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#dd77bb"
      }
    },
    // Python specific - Balanced colors
    {
      "name": "Python Self",
      "scope": [
        "variable.language.self.python"
      ],
      "settings": {
        "foreground": "#dd77dd",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Decorator",
      "scope": [
        "meta.decorator.python",
        "entity.name.function.decorator.python"
      ],
      "settings": {
        "foreground": "#ddaa77",
        "fontStyle": "bold"
      }
