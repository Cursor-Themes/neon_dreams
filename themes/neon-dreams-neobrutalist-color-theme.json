    {
      "name": "Python Magic Method",
      "scope": [
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#ee88ee"
      }
    },
    // JSON - Balanced colors
    {
      "name": "JSON Key",
      "scope": [
